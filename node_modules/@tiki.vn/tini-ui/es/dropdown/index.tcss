.tu-dropdown *{text-overflow:ellipsis;user-select:none}.dropdown-item{padding:0 var(--space-inset-10x)}.dropdown-item-selected{background:var(--color-list-background-hover)}.dropdown-item-content{height:40px;border-bottom:solid 1px var(--color-alias-outline-variant)}.dropdown-item checkbox{min-width:20px}.tu-dropdown-input-focus .tu-popup-content{top:0;left:0;right:0;bottom:0}.tu-dropdown-notfound{display:flex;flex-direction:column;align-items:center;justify-content:center}.tu-dropdown-notfound-title{font-weight:700;font-size:16px;margin-bottom:2px}.tu-dropdown-notfound-image{width:160px;height:160px;margin-bottom:12px;margin-top:32px}