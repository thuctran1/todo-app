.tu-icon-header{height:24px;width:24px}.tu-c-container{padding:16px;border-radius:4px}.tu-c-container .tu-c-head{display:flex;justify-content:space-between}.tu-c-container .tu-c-head-text{font-size:16px}.tu-c-head-days{display:flex;width:100%;padding-top:8px;padding-bottom:8px;margin-top:8px;margin-bottom:8px}.tu-c-head-days .tu-c-head-day{width:14.258%;font-size:16px;color:var(--color-alias-secondary-on-theme);text-align:center}.tu-c-body{width:100%;display:flex;flex-wrap:wrap}.tu-c-body .tu-c-date-wrapper{width:calc(14.285% - 2px);display:flex;flex-wrap:wrap;align-items:center;justify-content:center;border:1px solid transparent}.tu-c-body .tu-c-date-container{padding-top:16px;padding-bottom:16px;position:relative}.tu-c-body .tu-c-date-container .c-date{font-size:16px;text-align:center}.tu-c-body .tu-c-date-container .c-date-tag{position:absolute;left:50%;transform:translateX(-50%);bottom:0;font-size:10px;color:var(--color-alias-secondary-on-theme)}.tu-c-body .tu-c-date-wrapper.today{width:calc(14.285% - 2px);border:1px solid var(--color-alias-brand);border-radius:8px;color:var(--color-alias-brand);font-weight:700}.tu-c-body .tu-c-date-wrapper.inrange{background:rgba(178,217,250,.4);border-radius:0;border:1px solid transparent;color:var(--color-global-black);font-weight:400}.tu-c-body .tu-c-date-wrapper.selected{color:var(--color-alias-theme);font-weight:700;border-radius:0;border:1px solid transparent}.tu-c-body .tu-c-date-wrapper.selected.range{background:rgba(178,217,250,.4)}.tu-c-body .tu-c-date-wrapper.selected.start{border-top-left-radius:8px;border-bottom-left-radius:8px}.tu-c-body .tu-c-date-wrapper.selected.end{border-top-right-radius:8px;border-bottom-right-radius:8px}.tu-c-body .tu-c-date-wrapper.selected.end .tu-c-date-container,.tu-c-body .tu-c-date-wrapper.selected.range .tu-c-date-container,.tu-c-body .tu-c-date-wrapper.selected.start .tu-c-date-container{background:var(--color-alias-brand);width:100%;border-radius:8px}.tu-c-body .tu-c-date-wrapper.d