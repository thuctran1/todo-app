.tu-sidebar{width:100vw;height:100vh;display:flex;flex-direction:row}.tu-sidebar-bar{overflow:hidden}.tu-sidebar-bar-line{position:absolute;left:0;top:0;bottom:0;width:4px;overflow:hidden;font-size:0}.tu-sidebar-bar-placehold{flex:1 1 0%}.tu-sidebar-bar-content{display:flex;flex-direction:column;height:100%}.tu-sidebar-bar-content::-webkit-scrollbar{display:none}.tu-sidebar-bar-tab{position:relative;box-sizing:border-box;width:100%;padding:var(--space-inline-8x) var(--space-inline-16x)}.tu-sidebar-bar-tab__hover{opacity:.6}.tu-sidebar-bar-tab__active{font-size:var(--font-size-base)}.tu-sidebar-bar-tab__same{font-size:var(--font-size-base)}.tu-sidebar-bar-tab-arrow{margin-left:8px}.tu-sidebar-bar-tab-arrow.down{transform:rotate(0);transition:transform .2s linear}.tu-sidebar-bar-tab-arrow.up{transform:rotate(180deg);transition:transform .2s linear}.tu-sidebar-bar-tab-child{margin-left:-16px;margin-right:-16px;background:var(--color-alias-primary-on-reverse-theme);padding-top:8px;padding-bottom:8px;padding-left:24px;padding-right:16px}.tu-sidebar-bar-title{position:relative;display:flex;align-items:center;justify-content:space-between}.tu-sidebar-bar-title.center{justify-content:center}.tu-sidebar-bar-title-content{position:relative;display:flex;align-items:center}.tu-sidebar-bar-title-content .ml-8{margin-left:8px}.tu-sidebar-bar-title-content.vertical{flex-direction:column}.tu-sidebar-bar-title-content-image{width:24px;height:24px}.tu-sidebar-bar-title-content-text{display:block}.tu-sidebar-bar-title-content-text-small{font-size:var(--font-size-x-small)}.tu-sidebar-bar-title .tu-sidebar-bar-tab-badge.right{margin-left:8px}.tu-sidebar-bar-title .tu-sidebar-bar-tab-badge.top{position:absolute;top:0;right:0;transform:translate(60%,-50%)}.tu-sidebar-content-wrap{overflow:hidden;flex:1;height:100%;background-color:var(--background-panel)}.tu-sidebar-slides{height:100%}.tu-sidebar-slides.animate{transition-duration:.3s}