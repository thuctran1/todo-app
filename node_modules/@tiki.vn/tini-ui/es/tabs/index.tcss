.tu-tabs-tab-bar-wrap{width:100%}.tu-tabs-bar{position:relative;background-color:var(--background-panel);display:flex;align-items:stretch;width:100%;border-bottom:solid var(--color-alias-outline-variant);box-sizing:border-box;z-index:1}.tu-tabs-bar-sticky{position:sticky}.tu-tabs-bar-content{width:100%;display:flex;flex-direction:row;flex-wrap:nowrap;overflow:hidden}.tu-tabs-bar-content::-webkit-scrollbar{display:none}.tu-tabs-bar-content-wrap{position:relative;flex:1 1 auto;overflow:hidden;margin-bottom:-1px}.tu-tabs-bar-tab{display:flex;align-items:center;justify-content:center;flex:1 0 auto;font-size:var(--font-size-base);white-space:nowrap}.tu-tabs-bar-tab-mainTitle{position:relative;display:flex;align-items:center;justify-content:center}.tu-tabs-bar-tab-mainTitle-content{display:flex;flex-direction:column;align-items:center;justify-content:center}.tu-tabs-bar-tab-mainTitle-text{color:var(--color-tab-basic-text-inactive);display:block}.tu-tabs-bar-tab-mainTitle-text-active{color:var(--color-tab-basic-text-active);font-weight:var(--font-weight-bold)}.tu-tabs-bar-tab-mainTitle-text-small{font-size:var(--font-size-x-small)}.tu-tabs-bar-tab-mainTitle-image{width:24px;height:24px}.tu-tabs-bar-tab .tu-badge.right,.tu-tabs-bar-tab-badge.right{margin-left:2px}.tu-tabs-bar-tab .tu-badge.top,.tu-tabs-bar-tab-badge.top{position:absolute;top:0;right:0;transform:translate(15%,-50%)}.tu-tabs-bar-tab__hasSubTitle{margin-right:12px}.tu-tabs-bar-tab__hasSubTitle:last-child{margin-right:0}.tu-tabs-bar-tab-subTitle{padding:0 4px;font-size:var(--font-size-small);color:var(--color-tab-basic-text-inactive)}.tu-tabs-bar-tab-subTitle-active{color:var(--color-tab-basic-text-active)}.tu-tabs-bar-tab__hover{opacity:.6}.tu-tabs-bar-title{position:relative;display:flex;align-items:center;justify-content:center;padding:var(--space-stack-8x);box-sizing:border-box;flex:1;height:100%}.tu-tabs-bar-title__hasSubTitle{flex-direction:column;margin-right:0;text-align:center;border-bottom:0 none}.tu-tabs-bar-title-underline{position:absolute;left:50%;bottom:0;width:100%;height:1px;overflow:hidden;font-size:0;box-sizing:border-box;animation:underlineShow .2s linear 0s 1 forwards;background-color:var(--border-panel-active)}.tu-tabs-content-wrap{display:flex;flex-direction:row;align-content:stretch;overflow:hidden;height:auto;background-color:var(--color-alias-primary-on-reverse-theme)}.tu-tabs-content-wrap_H100{height:100%!important}.tu-tabs-slides{width:100%;display:flex}@keyframes underlineShow{0%{transform:scale(.1) translateX(-100%)}100%{transform:scale(1) translateX(-50%)}}